{
  "name": "@mojojs/sql",
  "version": "1.2.1",
  "description": "SQL generator",
  "keywords": [
    "mojojs",
    "database",
    "db",
    "sql"
  ],
  "author": "Sebastian Riedel <kraih@mojolicious.org>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojolicious/sql.js.git"
  },
  "bugs": {
    "url": "https://github.com/mojolicious/sql.js/issues"
  },
  "homepage": "https://mojojs.org",
  "scripts": {
    "build": "npx tsc --build ./",
    "build:clean": "npm run clean && npm run build",
    "build:coverage": "npm run build && npm run coverage",
    "build:lint": "npm run build && npm run lint",
    "build:node": "npm run build && node",
    "build:test": "npm run build && npm test",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf tsconfig.tsbuildinfo lib",
    "coverage": "c8 tap --no-coverage test/*.js",
    "coverage:ci": "c8 --reporter lcovonly tap --no-coverage test/*.js",
    "lint": "eslint \"test/*.js\" \"src/**/*.ts\" \"src/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "publish:minor": "npm version minor && npm publish",
    "publish:patch": "npm version patch && npm publish",
    "test": "tap --no-coverage test/*.js"
  },
  "exports": "./lib/sql.js",
  "types": "./lib/sql.d.ts",
  "type": "module",
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/tap": "^15.0.3",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "c8": "^7.11.3",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.0",
    "tap": "^16.3.0",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">= 16"
  }
}
